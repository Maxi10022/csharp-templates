{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_POSTGRES_CONNECTION_STRING"
  },
  "Smtp": {
    "Server": "YOUR_SMTP_SERVER",
    "Port": 587,
    "Username": "YOUR_SMTP_USERNAME",
    "Password": "YOUR_SMTP_USERS_PASSWORD",
    "EnableSsl": true
  },
  "Frontend": {
    "Root": "https://localhost:5173", 
    "Paths": {
      "EmailConfirmed": "/email-confirmed",
      "OAuthRedirect": "/dashboard"
    }
  },
  "OAuth": {
    "Google": {
      "ClientId": "YOUR_OAUTH_CLIENT_ID",
      "Secret": "YOUR_OAUTH_SECRET"
    }
  },
  "Branding": {
    "CompanyName": "Example Company",
    "PrimaryColor": "hsl(219, 66%, 41%)",
    "LogoUrl": null
  }
}
